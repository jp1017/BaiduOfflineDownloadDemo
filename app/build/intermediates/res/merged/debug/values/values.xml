<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="search_view"><attr format="string" name="hint"/><attr format="boolean" name="show_search_button"/></declare-styleable>
    <string name="app_name">BaiduOfflineDownloadDemo</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="MListView">
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:dividerHeight">1dip</item>
        <item name="android:listSelector">#00000000</item>
        <item name="android:background">#00000000</item>
        <item name="android:scrollbars">none</item>
        <item name="android:focusable">true</item>
        <item name="android:fadingEdge">none</item>
    </style>
</resources>